{
    "name": "oneguard/expression-language-extensions",
    "description": "Useful extensions to Symfony Expression Language",
    "type": "library",
    "keywords": ["Symfony", "Expression Language", "Extension"],
    "homepage": "https://github.com/OneGuardSolutions/expression-language-extensions",
    "authors": [
        {
            "name": "OneGuard",
            "homepage": "https://oneguard.solutions"
        },
        {
            "name": "Tomáš Pecsérke",
            "email": "tomas.pecserke@oneguard.email"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/expression-language": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "satooshi/php-coveralls": "^2.0"
    },
    "license": "MIT",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "OneGuard\\ExpressionLanguageExtensions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OneGuard\\ExpressionLanguageExtensions\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
